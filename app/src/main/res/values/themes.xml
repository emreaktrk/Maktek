<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="android:Theme.Holo.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/MaktekActionBar</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="AppTheme.Translucent" parent="android:Theme.Holo.Light.DarkActionBar">
