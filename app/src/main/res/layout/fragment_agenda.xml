<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AgendaFragment">

    <com.mobeta.android.dslv.DragSortListView
        android:id="@+id/fragment_agenda_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:cacheColorHint="@android:color/transparent"
        android:descendantFocusability="blocksDescendants"
        android:divider="@color/drawer_divider_color"
        android:dividerHeight="1dp"
        android:listSelector="@android:color/transparent"
        tools:listitem="@layout/cell_fair_hotels"/>

    <include
        android:id="@+id/fragment_agenda_emptyview"
        layout="@layout/empty_agenda"/>

</FrameLayout>
