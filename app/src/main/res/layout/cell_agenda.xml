<LinearLayout
    android:id="@+id/cell_agenda_clickview"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/prefered_listitem_extra_small_height_within_activity_space"
    android:clickable="true"
    android:gravity="center_vertical"
    android:orientation="horizontal">

    <View
        android:id="@+id/cell_agenda_saloonview"
        android:layout_width="10dp"
        android:layout_height="match_parent"/>

    <akturk.maktek.view.RobotoCondensedBoldTextView
        android:id="@+id/cell_agenda_name_textview"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/content_space"
        android:layout_weight="1"/>

    <View
        android:id="@+id/cell_agenda_handleview"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/ic_launcher"
        android:visibility="gone"/>
</LinearLayout>